<div class="image-project-info"><img src="{{projectImage}}" alt="project image project: {{name}}"></div>
<div class="project-info">
    <h3 class="project-title">{{name}}</h3>
    <p class="project-description">{{info}}</p>
    <p class="project-description">{{text}}</p>
    <div class="project-links">
        <a class="github-link" href="{{githubURL}}" target="_blank" aria-label="Github">
            <label>Github Repository: </label>
            <i class="fa fa-github"></i>
        </a>
        <a href="{{projectURL}}" target="_blank" aria-label="Project">
            <button>View Project</button>
        </a>
    </div>
</div>

